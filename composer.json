{
  "name": "clubdeuce/wp-share-this",
  "type": "library",
  "description": "An Share This library for WordPress.",
  "keywords": [ "wordpress", "php", "library", "sharethis", "maps" ],
  "homepage": "http://github.com/clubdeuce/wp-share-this",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Daryl Lozupone",
      "email": "daryl@clubdeuce.net",
      "role" :  "DevOp and Developer"
    }
  ],
  "autoload": {
    "classmap": [
      "wp-share-this.php"
    ]
  },
  "require": {
    "php": ">=5.4.0"
  },
  "require-dev": {
    "johnpbloch/wordpress": "*",
    "mockery/mockery": "*",
    "consolidation/robo": "*",
    "patchwork/jsqueeze": "*",
    "natxet/cssmin": "*",
    "lucatume/wp-browser": "*"
  },
  "extra": {
    "wordpress-install-dir": "wp"
  },
  "support": {
    "issues": "https://github.com/clubdeuce/wp-share-this/issues",
    "source":"https://github.com/clubdeuce/wp-share-this",
    "docs": "https://github.com/clubdeuce/wp-share-this/wiki"
  },
  "config": {
    "allow-plugins": {
      "johnpbloch/wordpress-core-installer": true
    }
  }
}
